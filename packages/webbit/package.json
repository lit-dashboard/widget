{
  "name": "@webbitjs/webbit",
  "version": "4.0.7",
  "description": "",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "generate-docs": "jsdoc -c jsdoc.conf.json",
    "build": "vite build && npm run build:types",
    "build:types": "tsc -p ./ --declaration --emitDeclarationOnly --declarationDir ./dist",
    "lint": "eslint src/**/*",
    "lint:format": "eslint src/**/* --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run lint && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@types/pubsub-js": "^1.8.3",
    "@webbitjs/store": "^4.0.2",
    "camel-case": "^4.1.2",
    "pubsub-js": "^1.9.4",
    "specificity": "^0.4.1",
    "type-fest": "^2.11.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbitjs/webbit.git"
  },
  "author": "Amory Galili",
